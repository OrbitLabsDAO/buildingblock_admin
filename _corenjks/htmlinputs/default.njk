<label  {% if field.name == "id"  %} class="d-none"  {% endif %}>{{ field.label }}</label>
<input type="text"  data-type="default"  class="form-control form-control-user  {% if field.name == "id"  %} d-none  {% endif %}>" 
       name="inp-{{ field.name }}" 
       id="inp-{{ field.name }}" 
       data-foriegnTable="{{ foreignTable }}"
       data-foriegnId="{{ foreignId }}"
       aria-describedby="{{ field.name }}" value="" {% if disabled %}disabled{% endif %} 
       placeholder="Unsupported Type: {{ field.datatype.variant }}"
       {% if field.isRequired %}required{% endif %}>
<span class="text-danger d-none" id="error-{{field.name}}"></span> 
