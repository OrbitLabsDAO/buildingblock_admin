<label  {% if field.name == "id" or field.name == "cfImageUrl"  %} class="d-none"  {% endif %}>{{ field.label }}</label>
<textarea  data-type="text"  class="form-control form-control-user  {% if field.name == "id" or field.name == "cfImageUrl"  %} d-none  {% endif %}>" 
name="inp-{{ field.name }}" 
id="inp-{{ field.name }}"  
data-foreigntable="{{ foreignTable }}"
data-foreignId="{{ foreignId }}"
rows="4" cols="50" aria-describedby="{{ field.name }}" {% if disabled %}disabled{% endif %}
{% if field.isRequired %}required{% endif %}>
</textarea>
<span class="text-danger d-none" id="error-{{field.name}}"></span>  
